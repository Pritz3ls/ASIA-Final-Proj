// Course API
# Get All Courses Data
http://localhost:3000/api/course/
Sample: [
    {
        "course_id": 1,
        "course_name": "Information Technology"
    },
    {
        "course_id": 2,
        "course_name": "Computer Science"
    },
    {
        "course_id": 3,
        "course_name": "Business Administration"
    }
]

# Get Courses Data by ID
http://localhost:3000/api/course/?/ ## the symbol ? is where you will specify the ID
Sample: [
    {
        "course_id": 1,
        "course_name": "Information Technology"
    }
]

# Get Total Students Count from a specific Course by ID
http://localhost:3000/api/course/students/?/ ## the symbol ? is where you will specify the ID
Sample: [
    {
        "course_total_students": 150,
        "course_id": 1,
        "course_name": "Information Technology"
    }
]

