# Get Total Counts of All Students
http://localhost:3000/api/students/count
Sample: [
    {
        "total": 500
    }
]

# Get a Specific Student using ID
http://localhost:3000/api/students/? ## the symbol ? is where you will specify the ID
Sample: {
    "student_id": 1,
    "first_name": "Sarah",
    "last_name": "Farmer",
    "course_id": 1,
    "date_of_birth": "2006-05-24T16:00:00.000Z",
    "email": "sarah.farmer0@example.com"
}

